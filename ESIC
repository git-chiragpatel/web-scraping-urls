import requests
from bs4 import BeautifulSoup
from selenium import webdriver

driver = webdriver.Chrome()
driver.get("https://www.esic.nic.in/recruitments")

driver.implicitly_wait(10)
page_source = driver.page_source

soup = BeautifulSoup(page_source, 'html.parser')

recruitments_section = soup.find('div', {'class': 'cms-content'}).find('ul')

recruitments_list = recruitments_section.find_all('li')

for recruitment in recruitments_list:
    print(recruitment.text)

driver.quit()
